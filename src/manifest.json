{
  "name": "Emoty",
  "version": "2.0.0",
  "manifest_version": 2,
  "icons": {
    "16": "images/icons/16.png",
    "48": "images/icons/48.png",
    "128": "images/icons/128.png"
  },
  "permissions": [
    "clipboardWrite"
  ],
  "browser_action": {
    "default_icon": "images/icons/19.png",
    "default_title": "Emoty",
    "default_popup": "popup.html"
  }
}
